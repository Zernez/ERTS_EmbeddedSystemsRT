class VideoStreaming : public VideoConnected
{
public:
	static VideoCaptureState* Instance();

	virtual void Stop(VideoCapture* context) override;
protected:
    VideoStreaming();
    ~VideoStreaming();
private:
    static VideoCaptureState* instance_;
};
#endif
